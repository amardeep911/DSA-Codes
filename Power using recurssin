#include<iostream>

using namespace std;

int fact(int m, int n){
    if(n==0){
        return 1;
    }
    else return fact(m, n-1) * m;
}
int main(){
    int r;
    r = fact(2, 3);
    printf("%d", r);
}

//There also a faster version by taking two multipliction at one time. By doing this the multiplication reduces to 6 rather than 9 hence fast.
